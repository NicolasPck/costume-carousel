<%= render "shared/navbar" %>
<h1>Create a new costume</h1>

<%= simple_form_for [@costume] do |f| %>
  <%= f.input :name %>
  <%= f.input :size %>
  <%= f.input :price_per_day%>
  <%= f.input :category %>
  <%= f.input :description %>
  <%= f.input :photo, as: :file %>
  <%= f.button :submit, class: "btn btn-primary" %>
<% end %>
